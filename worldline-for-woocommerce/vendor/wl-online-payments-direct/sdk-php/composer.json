{
    "name": "wl-online-payments-direct/sdk-php",
    "type": "library",
    "description": "PHP SDK to communicate with the payment platform server-to-server API",
    "keywords": ["SDK"],
    "license": "MIT",
    "authors": [
        {
            "name": "OnlinePayments"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "robtimus/multipart": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9"
    },
    "autoload": {
        "psr-4": {
            "OnlinePayments\\Sdk\\": ["src/OnlinePayments/Sdk", "lib/OnlinePayments/Sdk"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OnlinePayments\\Sdk\\": ["tests/OnlinePayments/Sdk"]
        }
    }
}
