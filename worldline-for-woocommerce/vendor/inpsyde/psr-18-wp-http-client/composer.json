{
    "name": "inpsyde\/psr-18-wp-http-client",
    "description": "A PSR-18 compatible wrapper for WordPress HTTP requests",
    "minimum-stability": "dev",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Inpsyde GmbH",
            "email": "hello@inpsyde.com",
            "role": "Company"
        },
        {
            "name": "Anton Ukhanev",
            "email": "xedin.unknown@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0 | ^8.0",
        "psr\/http-client": "^1.0",
        "psr\/http-factory": "^1.0",
        "psr\/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit\/phpunit": "^6.0 | ^7.0 | ^8.0 | ^9.0",
        "johnpbloch\/wordpress-core": "^5.0",
        "squizlabs\/php_codesniffer": "^3.4",
        "inpsyde\/php-coding-standards": "dev-main",
        "brain\/monkey": "2.*",
        "voku\/httpful": "^2.4"
    },
    "provide": {
        "psr\/http-client-implementation": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Syde\\Vendor\\Worldline\\Inpsyde\\Wp\\HttpClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Syde\\Vendor\\Worldline\\Inpsyde\\Wp\\HttpClient\\Test\\Func\\": "tests\/functional"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.1.x-dev"
        }
    }
}